                            <label class="pagination-current"><strong>{{currentpage}}/{{TotalPages}}</strong></label>
                            <button class="pagination-next" ng-disabled="currentpage == TotalPages" ng-click="GetCustomer(currentpage + 1)">Sau</button>
                            <button class="pagination-last" ng-disabled="currentpage == TotalPages" ng-click="GetCustomer(TotalPages)">Cuối cùng ></button>
                        </div>
                    </td>
                </tr>
            </table>
        </div>
    </div>


