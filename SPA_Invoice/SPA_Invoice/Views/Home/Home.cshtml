<style>
    #dateTimePopUp {
        width: 100%;
        height: 100% !important;
        top: 0;
        display: none;
    }

    .fill-line {
        border-bottom: 1px solid #e8d4d4;
        left: 0;
        right: 0;
        position: absolute;
    }

    svg.icon {
        width: 16px;
        height: 16px;
        fill: currentColor;
    }

    .getting-started {
        padding: 16px 0;
    }

    .btn.btn-link {
        text-decoration: unset;
    }

    .btn btn-link:hover {
        text-decoration: unset !important;
    }
    /*view started*/
    .scroll-y.noscroll-x.fill.body.scrollbox.zerotop {
        overflow-y: auto;
        -webkit-overflow-scrolling: touch;
        padding: 0 20px 50px;
        overflow-x: auto;
    }

    .close-lg {
        font-size: 60px;
        font-weight: 300;
    }

    .gs-header.gradient-border-bottom {
        border-bottom: 1px solid #eee;
        border-image: linear-gradient(to right,#fff 0,#eee 25%,#eee 50%,#eee 75%,#fff 100%);
        border-image-slice: 1;
        margin-bottom: 40px;
    }

    .gs-new-container.gs-new-featured {
        max-width: 768px;
        display: flex;
        margin: auto;
        margin-bottom: 30px;
    }

    a.featured-item.gs-video-section.text-left.gs-invoice-video {
        background-color: #f2f7fe;
        flex-grow: 1;
        flex-basis: 0;
        padding: 40px 35px 20px;
        margin-right: 20px;
        color: unset;
    }

    a.featured-item.gs-mobile-section {
        background-color: #f2f7fe;
        flex-grow: 1;
        flex-basis: 0;
        padding: 40px 35px 20px;
    }

    .gs-mobile-section:visited {
        color: unset;
    }

    .getting-started-help.product-help-opts {
        padding-top: 90px;
        padding-bottom: 30px;
        padding-left: 30px;
        padding-top: 60px;
        padding-bottom: 0;
    }

    .getting-started-help a {
        color: #222;
        transition: color .2s ease-in;
    }

        .getting-started-help a:hover {
            color: #2a90fb;
        }

    svg.icon.icon-gs-cta {
        width: 120px;
        height: 120px;
    }

    .font-large {
        font-size: 16px;
    }

    .col-lg-4.text-center {
        flex: 0 0 33.33333%;
        max-width: 33.33333%;
    }

    svg.icon-lg {
        fill: #375505;
        width: 30px;
        height: 25px;
    }
    .cursor-move{
        cursor: move!important;
    }
   
</style>
