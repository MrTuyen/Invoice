    }

    .sidenav {
        height: 100%;
        width: 0;
        position: fixed;
        z-index: 5;
        top: 0;
        right: 0;
        overflow-x: hidden;
        overflow-y: scroll;
        background-color: #ffffff;
        transition: 0.5s;
        padding-top: 10px;
    }

        .sidenav a {
            text-decoration: none;
            font-size: 25px;
            color: #818181;
            display: block;
            transition: 0.3s;
        }

            .sidenav a:hover {
                color: #f1f1f1;
            }

        .sidenav .closebtn {
            position: absolute;
            top: 0;
            right: 1px;
            padding: 0 10px;
            font-size: 36px;
        }

            .sidenav .closebtn:hover {
                position: absolute;
                color: #000000;
            }
</style>
<script>

    function openSynergyBar() {
        document.getElementById("mySidenav").style.display = "block";
        setTimeout(function () {
            $('.full-overlay').css({ 'z-index': 4, 'opacity': .5 }).click(closeSynergyBar);
        });
    }

    function closeSynergyBar() {
        document.getElementById("mySidenav").style.display = "none";
        $('.full-overlay').css({ 'z-index': -1, 'opacity': 0 });
    }

</script>

