            || user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.HOADONTIENDIEN
            || user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.HOADONBANHANG
            || user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.PHIEUXUATKHO)
        {
            <a href="javascript:;" ng-if="CURRENT_USER_ROLE.includes(USER_ROLE.THEM_MOI_HOA_DON.toString())" ng-click="ModalInvoice('', 1)" data-toggle="modal" data-target=".modal-invoice"><span class="fa fa-plus"></span> Hóa đơn</a>
        }
        else if (user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.HOADONTIENNUOC)
        {
            <a href="javascript:;" ng-if="CURRENT_USER_ROLE.includes(USER_ROLE.THEM_MOI_HOA_DON.toString())" ng-click="ModalInvoice('', 1)" data-toggle="modal" data-target=".modal-invoice"><span class="fa fa-plus"></span> Hóa đơn</a>
        }
        else
        {
            <a href="javascript:;" ng-if="CURRENT_USER_ROLE.includes(USER_ROLE.THEM_MOI_HOA_DON.toString())" ng-click="ModalReceipt('', 1)" data-toggle="modal" data-target=".modal-receipt"><span class="fa fa-plus"></span> Hóa đơn</a>
        }
        <a href="javascript:;" ng-click="ModalCustomer('', 1)" data-toggle="modal" data-target=".modal-customer"><span class="fa fa-plus"></span> Khách hàng</a>
        @if (user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.HOADONTIENNUOC)
        {
            <a href="javascript:;" ng-click="ModalProduct('', 1)" data-toggle="modal" data-target=".modal-product"><span class="fa fa-plus"></span> Biểu giá nước</a>
        }
        else if (user.USINGINVOICETYPE == (int)DS.Common.Enums.EnumHelper.AccountObjectType.HOADONTIENDIEN)
        {
            <a href="javascript:;" ng-click="ModalProduct('', 1)" data-toggle="modal" data-target=".modal-product"><span class="fa fa-plus"></span> Biểu giá điện</a>
        }
        else
        {
            <a href="javascript:;" ng-click="ModalProduct('', 1)" data-toggle="modal" data-target=".modal-product"><span class="fa fa-plus"></span> Hàng hóa, dịch vụ</a>
        }
    </div>
</div>

