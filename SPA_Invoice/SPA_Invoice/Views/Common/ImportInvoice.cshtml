@using DS.BusinessObject.Account;
@{
    AccountBO user = (AccountBO)Session[DS.Common.Helpers.ConfigHelper.User];
}
<style>
    .footer-import-invoice {
        background: #3E3F3E 0% 0% no-repeat padding-box;
        border: 1px solid #D9DEDB;
    }

    .btn-outline-secondary {
        color: black;
    }

    .btn-outline-secondary:hover {
        color: black;
        background: white;
    }

    .success-result {
        color: #2CA01C;
    }

    .error-result {
        color: deeppink;
    }

    .success-result i, .error-result i {
        font-size: 22px;
    }

    .your-field {
        min-width: 100%;
        background-color: white;
        border-color: #D9DEDB;
        min-height: 35px;
        font-size: 14px;
    }

    .your-field + button {
        background-color: white;
        border-color: #D9DEDB;
        min-height: 35px;
        font-size: 14px;
    }

    .your-field:hover, .your-field + button:hover {
        border-color: #D9DEDB;
    }

    .table-bordered {
        border: none;
    }

    #searchInvoice {
        background: #fff center right 12px no-repeat padding-box url(/Images/ic_search_48px.svg);
    }

    .font-color-background {
        padding: 9px 9px;
        font-size: 90%;
        color: #e83e8c;
        background-color: #f9f2f4;
        white-space: nowrap;
        border-radius: 6px;
    }

    div.dropdown-menu.open {
        max-height: 314px !important;
        overflow: hidden;
    }

    ul.dropdown-menu.inner {
        max-height: 260px !important;
        overflow-y: auto;
    }

    .ddl_finance {
        /* position: relative; */
        padding: 2px 5px 2px 4px;
        color: #333 !important;
        -webkit-appearance: none;
        background-image: url(/Images/arrow.png);
        background-position: 88px;
        background-repeat: no-repeat;
        text-indent: 0.01px;
        background-position: 100%;
        -webkit-padding-end: 30px !important;
        opacity: 1;
        padding-inline-end: 20px;
        -moz-appearance: none;
        border: #B7B7B7 solid 1px;
        appearance: none;
        background-size: contain;
        width: 100%;
        height: 26px;
        top: -2px;
        left: -3px;
    }

    .input-finance {
        padding-left: 5px;
        width: 100%;
        border: #B7B7B7 solid 1px;
        padding: 2px 5px;
        width: 100%;
    }

    .bootstrap-select > .dropdown-toggle {
        display: -ms-inline-flexbox;
        -ms-flex-align: center;
        -ms-flex-pack: justify;
        padding: 2px 5px 2px 4px;
        color: #333 !important;
        -webkit-appearance: none;
        /*background-image: url(/Images/dropdown-list.png);*/
        background-position: 88px !important;
        background-repeat: no-repeat;
        text-indent: 0.01px;
        background-position: 100%;
        -webkit-padding-end: 30px !important;
        opacity: 1;
        padding-inline-end: 20px;
        -moz-appearance: none;
        appearance: none;
        background-size: contain;
        height: 26px;
        width: 100%;
        cursor: pointer;
        text-align: left;
        height: 33px;
        display: block;
        white-space: nowrap;
        line-height: 28px;
        color: rgb(68, 68, 68);
        background-clip: padding-box;
        user-select: none;
        text-decoration: none;
        border-radius: 2px;
    }

    .bootstrap-select .dropdown-toggle .filter-option-inner-inner {
        display: block;
        white-space: nowrap;
        text-overflow: ellipsis;
        overflow: hidden;
    }

    .dropdown-toggle::after {
        display: inline-block;
        margin-left: 12px;
        vertical-align: 2px;
        content: "";
        border-top: .3em solid;
        border-right: .3em solid transparent;
        border-bottom: 0;
        border-left: .3em solid transparent;
    }

    .slect-box-ac > div.dropup .dropdown-toggle::after {
        display: inline-block;
        margin-left: 14px;
        vertical-align: 3px;
        content: "";
        border-top: 0;
        border-right: .3em solid transparent;
        border-bottom: .3em solid;
        border-left: .3em solid transparent;
    }

    span.ac-box.ac-selected {
        cursor: pointer;
        text-align: left;
        display: inline-block;
        white-space: nowrap;
        line-height: 32px;
        color: rgb(68, 68, 68);
        background-clip: padding-box;
        user-select: none;
        padding: 0 0;
        text-decoration: none;
        border-radius: 2px;
        height: 30px;
        margin-bottom: 2px;
        position: absolute;
    }

    .ac-selection-clear {
        cursor: pointer;
        position: absolute;
        right: 24px;
        font-size: 15px;
        font-weight: 200;
        top: 0px;
        display: inline-block;
    }

    .ac-selection-clear svg {
        width: 8px;
        height: 8px;
        vertical-align: middle;
    }

    .unauthenticate-warning {
        background-color: #ffead1;
        border-radius: 6px;
        margin-left: -3px;
        box-shadow: 0 0 black;
    }

    .import-invoice .list-inline-item:not(:last-child) {
        margin-right: 0;
    }
</style>

